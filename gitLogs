 void loop()[m
 {[m
[32m+[m
[32m+[m[32m    unsigned long currentTime = millis();[m
[32m+[m[32m    if ((currentTime - previouTime) >= eventTime)[m
[32m+[m[32m    {[m
[32m+[m[32m        startAlarm();[m
[32m+[m[32m        previouTime = currentTime;[m
[32m+[m[32m    }[m
