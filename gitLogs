[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    if (myMonth.val() != "") {[m
[32m+[m[32m                      if ([m
[32m+[m[32m                        myMonth.val() < 1 ||[m
[32m+[m[32m                        myMonth.val() > 12 ||[m
[32m+[m[32m                        isNaN(myMonth.val())[m
[32m+[m[32m                      ) {[m
[32m+[m[32m                        myAddAlarm.css({ color: "#babecc" }).attr("isok", "0");[m
