[32m+[m[32m          $(this).css({[m
[32m+[m[32m            "background-color": "#ae1100",[m
[32m+[m[32m            color: "white",[m
[32m+[m[32m          });[m
[32m+[m[32m        },[m
[32m+[m[32m        function () {[m
[32m+[m[32m          dw.text("");[m
[32m+[m[32m          dw.slideUp(150);[m
[32m+[m
